<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	xmlns:local="clr-namespace:ODM.Controls.Demos"
	x:Class="ODM.Controls.Demos.ButtonStyleDemo"
	d:DesignWidth="640" d:DesignHeight="480">
	<UserControl.Resources>
		<local:ButtonStyleDemoModel x:Key="ButtonStyleDemoModelDataSource" />
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" DataContext="{Binding Source={StaticResource ButtonStyleDemoModelDataSource}}">
		<Button Content="Button" HorizontalAlignment="Left" Height="100" Margin="90,124,0,0" VerticalAlignment="Top" Width="100" Style="{DynamicResource EllipseButtonStyle}" IsEnabled="False"/>
	</Grid>
</UserControl>
