    <RegionTargetDefinition Name="C-Antenna-TopLeft"
                            Domain_SaveCacheImageEnabled="true"
                            AroundRegionExtractor_SaveCacheImageEnabled="true"
                            AroundRegionProcessor_SaveCacheImageEnabled="true"
                            TargetRegionProcessor_SaveCacheImageEnabled="true"
                            RoiHalfWidth="70">
      <RegionTargetDefinition.RoiActualLine>
        <Line X1="2466"
              Y1="1534"
              X2="2466"
              Y2="1605" />
      </RegionTargetDefinition.RoiActualLine>
      <RegionTargetDefinition.AroundRegionExtractor>
        <DynThresholdRegionExtractor LightDark="Light"
                                     Offset="25">
           <DynThresholdRegionExtractor.ThresholdImageFilter>
             <GrayOpeningRectFilter MaskHeight="2" MaskWidth="0"/>
           </DynThresholdRegionExtractor.ThresholdImageFilter>
        </DynThresholdRegionExtractor>
      </RegionTargetDefinition.AroundRegionExtractor>
      <RegionTargetDefinition.AroundRegionProcessor>
        <CompositeRegionProcessor>
          <ConnectionRegionProcessor/>
          <OpeningRectangle1RegionProcessor Width="7" Height="7"/>
          <SelectShapeRegionProcessor Operation="And">
            <SelectShapeEntry Feature="Width" Min="80" Max="100"/>
            <SelectShapeEntry Feature="Height" Min="20" Max="35"/>
            <SelectShapeEntry Feature="Area" Min="2000" Max="4000"/>
          </SelectShapeRegionProcessor>
          <OpeningRectangle1RegionProcessor Width="7" Height="7"/>
          <SmallestRectangle1RegionProcessor/>
          <DilationRectangle1RegionProcessor Width="31" Height="1"/>
        </CompositeRegionProcessor>
      </RegionTargetDefinition.AroundRegionProcessor>
      <RegionTargetDefinition.TargetRegionExtractor>
        <AntennaOfS1423RegionExtractor GrayOpeningMaskHeight="1"
                                       GrayOpeningMaskWidth="0"
                                       GrayDilationMaskHeight="2"
                                       GrayDilationMaskWidth="8"
                                       LightDark="Light"
                                       AreaMin="500"
                                       AreaMax="99999999"
                                       OpeningWidth="3"
                                       OpeningHeight="6"
                                       ClosingWidth="3"
                                       ClosingHeight="3"
                                       />
                                               
      </RegionTargetDefinition.TargetRegionExtractor>
    </RegionTargetDefinition>
